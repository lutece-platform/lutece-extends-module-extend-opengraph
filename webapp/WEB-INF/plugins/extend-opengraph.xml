<?xml version="1.0" encoding="UTF-8"?><plug-in>

    <!-- Plugin Informations -->
    <name>extend-opengraph</name>
    <class>fr.paris.lutece.plugins.extend.modules.opengraph.service.OpengraphPlugin</class>
    <version>1.2.6-SNAPSHOT</version>
    <description>module.extend.opengraph.module.description</description>
    <provider>module.extend.opengraph.module.provider</provider>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/extend/modules/opengraph/extend-opengraph.png</icon-url>
    <copyright>Copyright 2001-2012 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>4.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>1</db-pool-required>
    
    <admin-features>
        <admin-feature>
            <feature-id>MANAGE_OPENGRAPH_SOCIALHUB</feature-id>
            <feature-title>module.extend.opengraph.adminFeature.manage_opengraph_socialhub.name</feature-title>
            <feature-description>module.extend.opengraph.adminFeature.manage_opengraph_socialhub.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/extend/modules/opengraph/GetManageOpengraphSocialHub.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/extend/modules/opengraph/extend-opengraph.png</feature-icon-url>
            <feature-documentation-url>jsp/admin/documentation/AdminDocumentation.jsp?doc=admin-extend-opengraph</feature-documentation-url>
        </admin-feature>
    </admin-features>
    
    <!-- RBAC Resources -->
    <rbac-resource-types>
        <rbac-resource-type>
            <rbac-resource-type-class>
                fr.paris.lutece.plugins.extend.modules.opengraph.service.OpengraphResourceIdService
			</rbac-resource-type-class>
        </rbac-resource-type>
    </rbac-resource-types>
</plug-in>